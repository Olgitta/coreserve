{
  "name": "coreserve",
  "version": "1.0.0",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "node ./bin/www",
    "lint": "eslint src/",
    "lintest": "eslint test/",
    "lintest:fix": "eslint test/ --fix",
