    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.1",
    "mysql2": "^3.11.5",
    "sequelize": "^6.37.5",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@faker-js/faker": "^9.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
